{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Uvicorn: app.main",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--reload", "--port", "8000"],
      "env": { "PYTHONPATH": "${workspaceFolder}" },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}

